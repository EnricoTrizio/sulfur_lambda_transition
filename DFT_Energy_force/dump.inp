&GLOBAL
  PRINT_LEVEL MEDIUM
  WALLTIME 1000000000
  PROJECT S_512
  RUN_TYPE ENERGY_FORCE
  SEED 300
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  STRESS_TENSOR ANALYTICAL
  &DFT
    CHARGE 0
    MULTIPLICITY 1
    BASIS_SET_FILE_NAME  BASIS_MOLOPT
    POTENTIAL_FILE_NAME  GTH_POTENTIALS
    WFN_RESTART_FILE_NAME S8_aimd-RESTART.wfn 
    &MGRID
      CUTOFF 350
      REL_CUTOFF 50
    &END MGRID
    
    &SCF
     SCF_GUESS ATOMIC
      EPS_SCF 1.0E-6
      MAX_SCF 300
      ADDED_MOS 50
      CHOLESKY INVERSE
      &DIAGONALIZATION ON
        ALGORITHM STANDARD
      &END DIAGONALIZATION
      &MIXING T
        METHOD BROYDEN_MIXING
        ALPHA 0.2
        BETA 0.5
        NBROYDEN 16
        &END MIXING
      &SMEAR ON
        METHOD FERMI_DIRAC
          ELECTRONIC_TEMPERATURE [K] 300
      &END SMEAR
      &PRINT
        &RESTART OFF
        &END RESTART
      &END PRINT
    &END SCF
    
    &XC
     &XC_FUNCTIONAL PBE
     &END XC_FUNCTIONAL
     &vdW_POTENTIAL
     DISPERSION_FUNCTIONAL PAIR_POTENTIAL
      &PAIR_POTENTIAL
        TYPE DFTD3
        PARAMETER_FILE_NAME dftd3.dat
        REFERENCE_FUNCTIONAL PBE
        CALCULATE_C9_TERM T
      &END PAIR_POTENTIAL
     &END vdW_POTENTIAL
    &END XC

      &PRINT
        &E_DENSITY_CUBE
        &END E_DENSITY_CUBE
        &MULLIKEN
        &END  MULLIKEN
      &END PRINT

 #   &KPOINTS
 #     FULL_GRID
 #     SCHEME MONKHORST-PACK 1 1 1
 #     SYMMETRY OFF
 #   &END KPOINTS
  &END DFT

  &SUBSYS
    &CELL
      ABC 24.8335586364  24.8335586364 24.8335586364
      PERIODIC XYZ
    &END CELL
    &TOPOLOGY
       COORD_FILE_NAME dump.xyz
       COORD_FILE_FORMAT XYZ
    &END TOPOLOGY

    &KIND S
      BASIS_SET  DZVP-MOLOPT-SR-GTH-q6
      POTENTIAL GTH-PBE-q6
      ELEMENT S
    &END KIND

   &END SUBSYS

&PRINT
  &FORCES ON
  &END FORCES
  &STRESS_TENSOR ON
  &END STRESS_TENSOR
&END PRINT
&END FORCE_EVAL
